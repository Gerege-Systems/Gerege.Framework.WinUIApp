<Page
    x:Class="SampleModule.PartnerPage.Partners"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="450" d:DesignWidth="800"
    d:Title="Partners"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid Loaded="Grid_Loaded">
        <Button Content="Back to home" HorizontalAlignment="Left" Margin="22,10,0,0" VerticalAlignment="Top" Width="151" Height="23" Click="Button_Click"/>
        <TextBox x:Name="TitleBox" HorizontalAlignment="Center" TextAlignment="Center" TextWrapping="Wrap" Text="Fetching partners list from server ..." VerticalAlignment="Top" Width="550" Margin="0,62,0,0"/>
        <StackPanel Orientation="Horizontal" Name="PartnersPanel" HorizontalAlignment="Center" Margin="0,120,0,0" VerticalAlignment="Top" Width="550" Height="300">
        </StackPanel>
    </Grid>
</Page>
